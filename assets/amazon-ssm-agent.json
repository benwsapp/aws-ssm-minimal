{
  "Profile": {
    "ShareCreds": false,
    "ShareProfile": "",
    "ForceUpdateCreds": false,
    "KeyAutoRotateDays": 0
  },
  "Agent": {
    "Audit": false,
    "TelemetryMetricsToSSM": false,
    "ContainerMode": true,
    "ForceFileIPC": true
  },
  "Identity": {
    "ConsumptionOrder": ["OnPrem"],
    "EC2": {
      "Disabled": true
    },
    "OnPrem": {
      "RegistrationKey": "RegistrationKey",
      "ShareProfile": "",
      "ShareFile": "/var/lib/amazon/ssm/runtimeconfig/managed-instance"
    }
  },
  "Mds": {
    "CommandRetryLimit": 15,
    "CommandWorkersLimit": 5
  },
  "Ssm": {
    "AssociationLogsRetentionDurationHours": 24,
    "RunCommandLogsRetentionDurationHours": 336,
    "SessionLogsRetentionDurationHours": 336,
    "SessionHandshakeTimeoutSeconds": 15,
    "SessionLogsDestination": "none",
    "LocalSessionDirectory": "/tmp"
  }
}
